{
  "name": "nxarch",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "build:all": "nx run-many --all --target=build",
    "lint:all": "nx run-many --all --target=lint --fix",
    "test:all": "nx run-many --all --target=test",
    "prepare": "husky install",
    "format": "nx format:write --all",
    "lint:affected": "nx affected:lint --base origin/develop",
    "test:affected": "nx affected:test --base origin/develop",
    "build:affected": "nx affected:build --base origin/develop",
    "c": "git-cz"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jscutlery/semver": "^2.25.2",
    "@nguniversal/express-engine": "^14.0.3",
    "@nrwl/cli": "14.4.3",
    "@nrwl/devkit": "14.4.3",
    "@nrwl/eslint-plugin-nx": "14.4.3",
    "@nrwl/jest": "14.4.3",
    "@nrwl/js": "14.4.3",
    "@nrwl/linter": "14.4.3",
    "@nrwl/nx-plugin": "^14.4.3",
    "@nrwl/workspace": "14.4.3",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@types/browser-sync": "^2.26.3",
    "@types/chokidar": "^2.1.3",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "ngx-deploy-npm": "^4.1.2",
    "nx": "14.4.3",
    "prettier": "^2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "tslib": "^2.0.0",
    "typescript": "~4.7.2"
  },
  "dependencies": {
    "@angular-devkit/schematics": "^14.1.0",
    "@nguniversal/builders": "^14.0.3",
    "@schematics/angular": "^14.1.0",
    "@swc/helpers": "~0.3.3",
    "browser-sync": "^2.27.10",
    "chokidar": "^3.5.3",
    "ts-morph": "^15.1.0",
    "wait-on": "^6.0.1"
  },
  "lint-staged": {
    "*.{ts,json,html,tsx,js}": "yarn format"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
