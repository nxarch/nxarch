    "tslib": "^2.0.0",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "*.{ts,json,html,tsx,js,yml}": "yarn format"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
