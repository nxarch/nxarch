{
  "name": "nxarch",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "build:all": "nx run-many --all --target=build",
    "lint:all": "nx run-many --all --target=lint --fix",
    "test:all": "nx run-many --all --target=test",
    "prepare": "husky install",
    "format": "nx format:write --all",
